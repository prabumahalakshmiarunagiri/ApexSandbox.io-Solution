public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    List<sObject> sObjList = new List<sObject>();

    if(accounts == null && contacts == null) return sObjList;

    if(!accounts.isEmpty() && accounts?.size()!=null) {
        for(Account acc : accounts) {
            sObjList.add(acc);
        }
    }
    if(!contacts.isEmpty() && contacts?.size()!=null) {
        for(Contact con : contacts) {
            sObjList.add(con);
        }
    }
    return sObjList;
}