public String registerStudent(String name, String phone, String email) {
    
    apxio__Student__c student = new apxio__Student__c(
        Name = name,
        apxio__Email__c = email,
        apxio__Phone__c = phone
    );
    insert student;

    return (String) [SELECT apxio__Registration_Number__c FROM apxio__Student__c WHERE Name=name AND apxio__Phone__c=phone AND apxio__Email__c=email];
    
}