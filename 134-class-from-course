public Id classFromCourse(String courseName) {
    
    apxio__Course__c course = [SELECT Id, Name FROM apxio__Course__c WHERE Name=: courseName];

    if(course.Id == null) return null;

    apxio__Class__c classObj = new apxio__Class__c();
    classObj.Name = courseName;
    classObj.apxio__Course__c = course.Id;
    insert classObj;

    return classObj.Id;
}