public void setAccountRating(Account a) {
    
    if(a.AnnualRevenue == null) return;

    if(a.AnnualRevenue <= 100000) {
        a.Rating = 'Cold';
    } else if(a.AnnualRevenue <= 500000 || a.AnnualRevenue > 100000) {
        a.Rating = 'Warm';
    }
    else if(a.AnnualRevenue => 500000){
        a.Rating = 'Hot';
    }
}