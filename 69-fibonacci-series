public List<Integer> fibonacciSeries(Integer n) {
    
    Integer a = 1;
    Integer b = 1;
    Integer c;

    List<Integer> fibonacci = new List<Integer> {1,1,2};

    if(n < 3) {
        return fibonacci.add(1,1,2);
    }

    for(Integer i=3; i<n; i++) {
        c = b;
        b = a;
        a = a + c;
        fibonacci.add(c);
    }
    return fibonacci;
}