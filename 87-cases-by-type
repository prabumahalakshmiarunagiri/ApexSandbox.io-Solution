public Map<String, List<Case>> casesByType(List<Case> cases) {
    
    Map<String, List<Case>> caseTypeMap = new Map<String, List<Case>>();

    for(Case c : cases) {
        if(c.Type != null) {
            if(caseTypeMap.containsKey(c.Type)) {
                caseTypeMap.put(c.Type, new List<Case>{c});
            }
        }
    }
}